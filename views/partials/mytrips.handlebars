<h1>My Trips</h1>
<container class="row myTripsContainer">
{{#each trips as |trip|}}

<div class="col-6">
<div class="card tripCard">
  <div class="card-header">
    {{trip.trip_name}}
  </div>
  <ul class="list-group list-group-flush">
    <li class="list-group-item">Attendees: {{trip.attendees}}</li>
    <li class="list-group-item">Trip Information: {{trip.trip_info}}</li> 
  </ul>
    <div class="btn-group" role="group" aria-label="Basic mixed styles example">
      <button type="submit" class="btn btn-primary viewTripBtn" data-id={{trip.id}}>View Trip</button>
    </div>
</div>
</div>

{{/each}}
</container>

<container class="row">
  <div class="col-12" id="newTripBtnDiv">
    <button type="submit" class="btn btn-success" id="newTripBtn">Create a New Trip!</button>
  </div>
</container>

<script type="text/javascript" src="/js/members.js"></script>